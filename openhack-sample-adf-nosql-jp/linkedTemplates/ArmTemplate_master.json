{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "データ ファクトリ名",
			"defaultValue": "openhack-sample-adf-nosql-jp"
		},
		"dstCosmos_connectionString": {
			"type": "secureString",
			"metadata": "'dstCosmos' の 'connectionString' のセキュリティで保護された文字列"
		},
		"srcSQL_connectionString": {
			"type": "secureString",
			"metadata": "'srcSQL' の 'connectionString' のセキュリティで保護された文字列"
		},
		"containerUri": {
			"type": "string",
			"metadata": "リンクされた ARM テンプレートを含むストレージ アカウントの URI"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "リンクされた ARM テンプレートを格納するストレージ アカウントへの SAS トークン"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "openhack-sample-adf-nosql-jp_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"dstCosmos_connectionString": {
						"value": "[parameters('dstCosmos_connectionString')]"
					},
					"srcSQL_connectionString": {
						"value": "[parameters('srcSQL_connectionString')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}