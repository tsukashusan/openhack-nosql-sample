{
	"name": "CopyRawfromSQLToCosmos",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "srcCartitem",
						"type": "DatasetReference"
					},
					"name": "srcCartitem"
				}
			],
			"sinks": [
				{
					"dataset": {
						"referenceName": "dstCartitem",
						"type": "DatasetReference"
					},
					"name": "dstCartitem"
				}
			],
			"transformations": [],
			"scriptLines": [
				"source(output(",
				"          CartItemId as string,",
				"          CartId as string,",
				"          ItemId as integer,",
				"          Quantity as integer,",
				"          DateCreated as timestamp",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     isolationLevel: 'READ_UNCOMMITTED',",
				"     format: 'table') ~> srcCartitem",
				"srcCartitem sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     deletable:false,",
				"     insertable:true,",
				"     updateable:false,",
				"     upsertable:false,",
				"     format: 'document',",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> dstCartitem"
			]
		}
	}
}