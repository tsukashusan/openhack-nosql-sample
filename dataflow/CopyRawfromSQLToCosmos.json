{
	"name": "CopyRawfromSQLToCosmos",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "srcOrders",
						"type": "DatasetReference"
					},
					"name": "order"
				}
			],
			"sinks": [],
			"transformations": [],
			"scriptLines": [
				"source(output(",
				"          OrderId as integer,",
				"          OrderDate as timestamp,",
				"          FirstName as string,",
				"          LastName as string,",
				"          Address as string,",
				"          City as string,",
				"          State as string,",
				"          PostalCode as string,",
				"          Country as string,",
				"          Phone as string,",
				"          SMSOptIn as boolean,",
				"          SMSStatus as string,",
				"          Email as string,",
				"          ReceiptUrl as string,",
				"          Total as decimal(18,2),",
				"          PaymentTransactionId as string,",
				"          HasBeenShipped as boolean",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     isolationLevel: 'READ_UNCOMMITTED',",
				"     format: 'table') ~> order"
			]
		}
	}
}